; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Car_game"
run/main_scene="uid://dl4l032e7tmky"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=60
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
GlobalVar="*res://global_var.gd"

[display]

window/size/viewport_width=1612
window/size/viewport_height=720
window/frame_pacing/android/swappy_mode=0
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")

[gui]

theme/custom_font="uid://cbwf465gr1onm"
theme/default_font_antialiasing=0

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

3d_physics/layer_1="Cars"
3d_physics/layer_2="terrain"
3d_physics/layer_3="objetos ligeros"
3d_physics/layer_4="traps"
3d_physics/layer_5="oil_trap"
3d_physics/layer_6="turret_sensor"
3d_physics/layer_7="projectil"
3d_physics/layer_8="checkpoints"
3d_physics/layer_9="action_icons"

[memory]

limits/message_queue/max_size_mb=64

[physics]

3d/physics_engine="Jolt Physics"

[rendering]

occlusion_culling/bvh_build_quality=1
textures/canvas_textures/default_texture_filter=0
